<div id="user-content" class="tab-content">
  <div id="user-list__header">
    <h2 style="font-size: 20px">ユーザー検索機能</h2>
  </div>
  <div id="home__user-list" class="full">
    <ul id="user-list">
    <% @users.each do |user| %>
      <li class="user">
        <div class="user-box">
          <img src="assets/sample.jpg">
          <h2 class="user-box__name"><%= user.name %></h2>
        </div>
      </li>
      <% end %>
    </ul>
  </div>
</div>
